mixin Faq(config, index)
  .accordion-item.border-top
    h2(id=`heading${config.id}`).accordion-header
      button.accordion-button.px-2(type='button' data-bs-toggle='collapse'  class=index == 0 ? false : 'collapsed'  data-bs-target=`#collapse${config.id}` aria-expanded='true' aria-controls=`collapse${config.id}`)
        span.mb-0.fw-bold.text-start.fs-1.text-1000=config.title
    .accordion-collapse.collapse(id=`collapse${config.id}` class=index == 0 ? 'show' : false  aria-labelledby=`heading${config.id}`  data-bs-parent='#accordionExample')
      .accordion-body.pt-0.px-2=config.description
        
mixin Faqs
  +Section.pb-md-10#faq
    .row.justify-content-center
      .col-md-8.col-lg-6.text-center.mb-3
        h6.fw-bold.fs-4.display-3.lh-sm.mb-5 Frequently asked questions
      
    .row.flex-center
      .col-lg-9
        #accordionExample.accordion
          each faq, index in faqs
            +Faq(faq, index)